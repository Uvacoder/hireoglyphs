{
  "name": "svg-icon-system",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run svg",
    "svg": "rm -r ./icons/* && npm run optimise && npm run sprite",
    "optimise": "svgo -f src -r -o icons --config=svgo.json",
    "sprite": "svg-sprite --config sprite.json './icons/./**/*.svg'"
  },
  "dependencies": {
    "svg-sprite": "^1.5.0",
    "svgo": "^1.3.1"
  }
}
