{
  "name": "svg-icon-system",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run svg",
    "svg": "npm run optimise && npm run sprites",
    "optimise": "svgo -f src -r -o dist --config=svgo.json",
    "sprites": "npm run sprites:24 && npm run sprites:test",
    "sprites:24": "svg-sprite-generate -d dist/24 -o dist/sprites-24.svg",
    "sprites:test": "svg-sprite-generate -d dist/tests -o dist/sprites-tests.svg"
  },
  "dependencies": {
    "svg-sprite-generator": "^0.0.7",
    "svgo": "^1.3.1"
  }
}
